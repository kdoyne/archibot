<%= form_for [@architect, @building] do |f| %>
  <%= f.label :name %><br>
  <%= f.text_field :name %><br>

  <%= f.label :address %><br>
  <%= f.text_field :address %><br>

  <%= f.label :year_built %><br>
  <%= f.text_field :year_built %><br>

  <%= f.label :photo_url %><br>
  <%= f.url_field :photo_url %><br>

  <%= f.submit %>
<%end%>